<odoo>

    <record id='wsf_noticias_secuencia_action' model='ir.actions.act_window'>
        <field name="name">Norep</field>
        <field name="res_model">wsf_noticias_secuencia</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="wsf_noticias_secuencia_root"
              parent="wsf_noticias_root"
              name="Secuencia"
              action="wsf_noticias_secuencia_action"/>

    <!-- Tree View -->
    <record id="wsf_noticias_secuencia_tree" model="ir.ui.view">
        <field name="name">Secuencia</field>
        <field name="model">wsf_noticias_secuencia</field>
        <field name="arch" type="xml">
            <tree multi_edit="1">
<!--                <field name="medio"/>-->
                <field name="ult_id"/>
                <field name="importancia"/>

            </tree>
        </field>
    </record>
    <!--fin tree-->



</odoo>